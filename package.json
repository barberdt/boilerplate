{
  "name": "gamerlink",
  "version": "0.0.1",
  "description": "The GamerLink application.",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"yarn run server:watch\" \"yarn run client:watch\"",
    "server:watch": "NODE_ENV=development && nodemon server/index.js --exec babel-node --watch server",
    "client:watch": "webpack --watch"
  },
